<% if (swagger.info) { %>
  <% var info = swagger.info %>
  <nop>
    # <%= info.title %>

    <% if (info.version) { %>
      <sup>`<%= info.version %>`</sup>
    <% } %>
  </nop>
  <%_ if (info.description) { _%>
    <%= swagger.info.description %>
  <%_ } _%>

  <% if (info.license) { %>
    ## License 
    [<%= info.license.name %>](<%= info.license.url %>)
  <% } %>

  <% if (info.contact) { %>
    ## Contact the developer
    <% Object.keys(info.contact).forEach(function(item) { %>
      <nop>
      - **<%= item %>** - <%= info.contact[item] %>
      </nop>
    <% }) %>
  <% } %>
<% } %>


<% if (swagger.externalDocs) { %>
  ## External Docs
  <nop>
    <%= swagger.externalDocs.description%>
    [<%= swagger.externalDocs.url%>](<%= swagger.externalDocs.url %>)
  </nop>
<% } %>

---

<% if (Array.isArray(swagger.servers)) { %>
  ## Servers
  <% swagger.servers.forEach(function(server) { %>
    <nop>
    - <%= server.url %>
    </nop>
  <% }) %>
<% } %>